pipeline {
    agent {
        node ('master')
    }
    stages {
        stage('clear ws'){
            steps{
            cleanWs()
            }
            }
        stage('Git Checkout') {
            steps {
              git branch: 'main', url: 'https://github.com/MenonSreenath/cm130_testing.git'
            }
          }
          
        stage ('read csv2') {
            steps {
             script{
              env.WORKSPACE = pwd()
                def version = readFile "${env.WORKSPACE}/products.csv"
                
                sh 'cat products.csv'
                }
        }
        }
        }
        }
            

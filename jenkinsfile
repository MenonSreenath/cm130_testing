pipeline {
    agent {
        node ('master')
    }
    stages {
        stage('clear ws'){
            steps{
            cleanWs()
            }
            }
        stage('Git Checkout') {
            steps {
              git branch: 'main', url: 'https://github.com/MenonSreenath/cm130_testing.git'
            }
          }
          
        stage ('read csv2') {
            steps {
                withFileParameter(name:'products.csv', allowNoFile: true)
                                    sh 'cat products.csv'
                                  }
                                }
                                }
                                }
                                }
              
         stage ('Run Python') {
            steps {
             script{
                sh 'python3 sshtest.py'
                }
        }
        }
        }
        }
            

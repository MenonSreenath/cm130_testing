pipeline {
    agent {
        node ('master')
    }
    stages {
        stage('Git Checkout') {
            steps {
              git branch: 'main', url: 'https://github.com/MenonSreenath/edu_project1.git'
            }
          }
          stage('read csv') {
            steps {
              script {
                 def fileBase64 = input message: 'Please provide a file', parameters: [base64File('file')]
                 node {
                     withEnv(["fileBase64=$fileBase64"]) {
                                                           sh 'echo $fileBase64 | base64 -d > myFile.txt'
        }
        }
        }
        }
        }
        }
        }
            

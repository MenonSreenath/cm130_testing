pipeline {
    agent {
        node ('master')
    }
    stages {
        stage('Git Checkout') {
            steps {
              git branch: 'main', url: 'https://github.com/MenonSreenath/edu_project1.git'
            }
          }
          stage('read csv') {
            steps {
              script {
                 
                        def inputCSVPath = input message: 'Upload file', parameters: [file(name: 'products.csv', description: 'Upload only CSV file')]
                        def csvContent = readFile "${products.csv}"
                        
                         echo ("CSV FILE PATH IS : ${inputCSVPath}")
                         echo("CSV CONTENT IS: ${csvContent}") 
        }
        }
        }
        }
        }
            

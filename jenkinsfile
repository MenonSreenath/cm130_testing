pipeline {
    agent {
        node ('master')
    }
    stages {
        stage('clear ws'){
            steps{
            cleanWs()
            }
            }
        stage('Git Checkout') {
            steps {
              git branch: 'main', url: 'https://github.com/MenonSreenath/cm130_testing.git'
            }
          }
          stage('read csv') {
            steps {
              script {
              properties([
                  parameters([
                    base64File(name: 'products.csv', description: "FIle to read")
                    ])
                    ])
                    
                    sh 'cat products.csv'
                    
        }
        }
        }
        }
        }
            
